{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "memory": 3008,
      "maxDuration": 10,
      "regions": ["sfo1", "iad1", "gru1"]
    },
    "api/**/*.py": {
      "runtime": "python3.9",
      "memory": 3008,
      "maxDuration": 10,
      "regions": ["sfo1", "iad1", "gru1"]
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
