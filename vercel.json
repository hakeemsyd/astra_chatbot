{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "memory": 1700,
      "maxDuration": 10
    },
    "api/**/*.py": {
      "runtime": "python3.9",
      "memory": 1700,
      "maxDuration": 10      
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
